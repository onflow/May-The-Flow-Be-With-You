# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key
SUPABASE_JWT_SECRET=your_jwt_secret

# AI Integration
ANTHROPIC_API_KEY=your_claude_api_key

# Silk Components (for commercial use)
SILK_LICENSE_KEY=your_silk_license_key

# Flow Blockchain Configuration
# Options: emulator (for local dev), testnet (for hackathon), mainnet (for production)
NEXT_PUBLIC_FLOW_NETWORK=testnet

# WalletConnect Project ID (get from https://cloud.walletconnect.com/)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

# Flow Contract Addresses (deployed to testnet)
NEXT_PUBLIC_MEMORY_VRF_CONTRACT=0xb8404e09b36b6623
NEXT_PUBLIC_MEMORY_ACHIEVEMENTS_CONTRACT=0xb8404e09b36b6623

# Flow Testnet Account (for deployment)
FLOW_TESTNET_ADDRESS=0x1234567890abcdef
FLOW_TESTNET_PRIVATE_KEY=your_testnet_private_key

# Development URLs (when using emulator)
# NEXT_PUBLIC_FLOW_ACCESS_NODE=http://127.0.0.1:8888
# NEXT_PUBLIC_FLOW_WALLET_DISCOVERY=http://localhost:8701/fcl/authn

# Testnet URLs (for hackathon submission)
# NEXT_PUBLIC_FLOW_ACCESS_NODE=https://rest-testnet.onflow.org
# NEXT_PUBLIC_FLOW_WALLET_DISCOVERY=https://fcl-discovery.onflow.org/testnet/authn
