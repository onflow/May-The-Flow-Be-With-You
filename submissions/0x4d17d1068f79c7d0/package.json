{
  "name": "memoreee",
  "version": "0.1.0",
  "private": true,
  "description": "A gamified memory training platform that awakens the dormant powers of human memory through classical techniques",
  "scripts": {
    "dev": "bun --bun next dev --turbopack",
    "dev:node": "next dev --turbopack",
    "dev:local": "bun scripts/dev/dev.js",
    "dev:flow": "bun scripts/dev/dev-flow.js",
    "build": "bun --bun next build",
    "build:node": "next build",
    "start": "bun --bun next start",
    "start:node": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "build:analyze": "ANALYZE=true bun --bun next build",
    "preview": "bun run build && bun run start",
    "clean": "rm -rf .next node_modules/.cache",
    "install:bun": "bun install",
    "install:npm": "npm install",
    "flow:emulator": "flow emulator start",
    "flow:dev-wallet": "flow dev-wallet",
    "flow:deploy": "flow project deploy --network emulator",
    "flow:deploy:testnet": "./scripts/deploy/deploy-testnet.sh",
    "flow:test": "flow scripts execute blockchain/scripts/test-vrf.cdc --network testnet",
    "test": "node blockchain/tests/run-all-tests.js",
    "test:vrf": "node blockchain/tests/vrf-integration.test.js",
    "test:network": "node blockchain/tests/network-detection.test.js",
    "test:cadence": "flow test",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "db:setup": "node scripts/setup-database.js",
    "db:verify": "node scripts/setup-database.js --direct",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod"
  },
  "dependencies": {
    "@onflow/fcl": "^1.12.2",
    "@onflow/types": "^1.0.5",
    "@onflow/util-encode-key": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.0",
    "@radix-ui/react-dropdown-menu": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.0",
    "@radix-ui/react-tabs": "^1.0.0",
    "@react-three/drei": "^9.88.0",
    "@react-three/fiber": "^8.15.0",
    "@silk-hq/components": "^0.9.3",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/auth-helpers-react": "^0.5.0",
    "@supabase/auth-ui-react": "^0.4.0",
    "@supabase/auth-ui-shared": "^0.1.0",
    "@supabase/supabase-js": "^2.38.0",
    "@supabase/ui": "^0.36.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "dotenv": "^16.5.0",
    "framer-motion": "^12.10.5",
    "lucide-react": "^0.263.1",
    "next": "15.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-spring": "^9.7.0",
    "react-use-gesture": "^9.1.3",
    "recharts": "^2.8.0",
    "three": "^0.158.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/three": "^0.158.0",
    "autoprefixer": "^10.4.20",
    "pino-pretty": "^13.0.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0",
    "bun": ">=1.0.0"
  }
}
